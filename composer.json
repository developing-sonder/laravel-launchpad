{
    "name": "developing-sonder/laravel-launchpad",
    "description": "Tool and command set for new and existing Laravel applications to spit up model factories and basic testing. ",
    "keywords": [
        "developing-sonder",
        "laravel-launchpad"
    ],
    "homepage": "https://github.com/developing-sonder/laravel-launchpad",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Matthew Moore",
            "email": "matthew.moore@developingsonder.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "illuminate/console": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "DevelopingSonder\\LaravelLaunchpad\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevelopingSonder\\LaravelLaunchpad\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevelopingSonder\\LaravelLaunchpad\\LaravelLaunchpadServiceProvider"
            ],
            "aliases": {
                "LaravelLaunchpad": "DevelopingSonder\\LaravelLaunchpad\\LaravelLaunchpadFacade"
            }
        }
    }
}
